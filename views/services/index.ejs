      <!-- ========== Start About Section ========== -->
      <section class="section-about">
        <div class="container ">
          <div class="section-common-heading">About Kushwaha IT Services</div>
          <div class="section-common-subheadoing"> Our commitment to excellence, innovation, and client success sets us apart.
         </div>
        </div>
 
        <div class="container grid grid-three--cols">
          <div class="about-div">
            <div class="icon">
              <img src="/images/research.png" alt="research image" />
            </div>
            <h3 class="section-common--title">Research</h3>
            <p>Research is the compass that guides innovation; it transforms questions into insights, challenges into opportunities, and ideas into impactful solutions.</p>
          </div>

          <div class="about-div">
            <div class="icon">
              <img src="/images/design.png" alt="research image" />
            </div>
            <h3 class="section-common--title">Design</h3>
            <p>Design is the art of transforming ideas into impactful experiences, where creativity meets purpose to shape the world around us.</p>
          </div>

          <div class="about-div">
            <div class="icon">
              <img src="/images/Coding.png" alt="research image" />
            </div>
            <h3 class="section-common--title">Development</h3>
            <p>Development is the craft of turning possibilities into reality, where innovation meets precision to build the future one line of code at a time.</p>
          </div>
        </div>
      </section>

      
      <!-- ========== End About Section ========== -->


      <!-- ========== Start services Section ========== -->

      <section class="section-course">
        <div class="container ">
          <div class="section-common-heading">Explore our Services</div>
          <div class="section-common-subheadoing"> Discover a variety of services across different platform.
         </div>
        </div>

        <div class="container grid grid-four--cols">
          <% for(let service of allServices) { %>
          <div class="course-div">
            <div class="icon">
              <i class="<%= service.icon[0]%> <%= service.icon[1] %>"></i>
            </div>
            
            <h3 class="section-common--title"><%=service.title %></h3>
            <h3 class="section-common--title"> Features Are : </h3>
             <% for(let feature of service.features) { %>
              
              <li><%= feature %></li>
            
               <br>
              <% } %>
              
              <a href="/services/<%=service.id %>" class="btn btn-white">Get Started ...  </a>
          </div>
       <% } %>
        </div>
      </section>
      
      <!-- ========== End services Section ========== -->

      <!-- ========== Start Why we choose ========== -->
